#use std;

fn move(x:i32, y:i32)
{
    putint(x); putch(32); putint(y); putch(44); putch(32);
}

fn hanoi(n:i32, one:i32, two:i32, three:i32)
{
    if (n == 1)
    {
        move(one, three);
    }
    else {
        hanoi(n - 1, one, three, two);
        move(one, three);
        hanoi(n - 1, two, one, three);
    }
}

fn main()->i32
{   
    timer_start(21);
    let n:i32 = getint();
    while (n > 0) {
        hanoi(getint(), 1, 2, 3);
        putch(10);
        n = n - 1;
    }
    timer_stop(28);
    ret 0;
}
