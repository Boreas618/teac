use std;

let n:i32;
fn Merge(array: [i32; 10], low:i32, middle:i32, high:i32)->i32
{
    let n1:i32;
    n1 = middle - low + 1;
    let n2:i32;
    n2 = high - middle;
    let L: [i32; 10];
    let R: [i32; 10];
    let i:i32;
    i = 0;
    let j:i32;
    j = 0;
    let temp:i32;
    while i < n1{
        temp = i + low;
        L[i] = array[temp];
        i = i + 1;        
    }
    while j < n2 {
        temp = j + middle  +1;
        R[j] = array[temp];
        j = j + 1;        
    }
    i = 0;
    j = 0;
    let k:i32;
    k = low;
    while (i!=n1) && (j!= n2)
    {   
        if L[i] < (R[j] + 1) {
            array[k] = L[i];
            k = k + 1;
            i = i + 1;
        }
        else {
            array[k] = R[j];
            k = k + 1;
            j = j + 1;
        }
    }
    while i < n1 {
        array[k] = L[i];
        k = k + 1;
        i = i + 1;
    }
    while j < n2 {
        array[k] = R[j];
        k = k + 1;
        j = j + 1;
    }
    ret 0;
}
 
fn MergeSort(array: [i32; 10], p:i32, q:i32)->i32
{
    if p < q
    {
        let mid:i32;
        mid = (p+q)/2;
        let tmp:i32;
        tmp = MergeSort(array, p, mid);
        tmp = mid + 1;
        tmp = MergeSort(array, tmp, q);
        tmp = Merge(array,p, mid, q);
    }
    ret 0;
}

fn main()->i32 {
    std::timer_start(71);
    n = 10;
    let a: [i32; 10];
    a[0] = 4;
    a[1] = 3;
    a[2] = 9;
    a[3] = 2;
    a[4] = 0;
    a[5] = 1;
    a[6] = 6;
    a[7] = 5;
    a[8] = 7;
    a[9] = 8;
    let i:i32;
    i = 0;
    let tmp:i32;
    tmp = n - 1;
    i = MergeSort(a, i, tmp);
    while i < n {
        tmp = a[i];
        std::putint(tmp);
        tmp = 10;
        std::putch(tmp);
        i = i + 1;
    }
    std::timer_stop(97);
    ret 0;
}
