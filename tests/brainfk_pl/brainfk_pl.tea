#use std;

let TAPE_LEN:int = 65536;
let BUFFER_LEN:int = 32768;
let tape[65536]:int;
let program[32768]:int;
let ptr:int = 0;

fn read_program() {
  let i:int = 0;
  let len:int = getint();
  while (i < len) {
    program[i] = getch();
    i = i + 1;
  }
  program[i] = 0;
}

fn interpret(input[65536]:int) {
  let cur_char:int;
  let loop:int;
  let i:int = 0;
  while (input[i]!=0) {
    cur_char = input[i];
    if (cur_char == 62) {
      ptr = ptr + 1;
    }
    if (cur_char == 60) {
      ptr = ptr - 1;
    }
    if (cur_char == 43) {
      tape[ptr] = tape[ptr] + 1;
    }
    if (cur_char == 45) {
      tape[ptr] = tape[ptr] - 1;
    }
    if (cur_char == 46) {
      putch(tape[ptr]);
    }
    if (cur_char == 44) {
      tape[ptr] = getch();
    }
   if ((cur_char == 93) && (tape[ptr]!=0)) {
      loop = 1;
      while (loop > 0) {
        i = i - 1;
        cur_char = input[i];
        if (cur_char == 91) {
          loop = loop - 1;
        }
        if (cur_char == 93) {
          loop = loop + 1;
        }
      }
    }
    i = i + 1;
  }
}

fn main()->int {
  timer_start(76);
  read_program();
  interpret(program);
  putch(10);
  timer_stop(80);
  ret 0;
}
