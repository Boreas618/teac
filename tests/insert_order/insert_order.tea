use std;

let N:i32;
fn insert(a: [i32; 11],x:i32)->i32
{
    let p:i32;
    let i:i32;
    p=0;

    while (x>a[p])&&(p<N)
    {
        p=p+1;
    }

    i=N;
    while i>p
    {
        let j:i32 = i - 1;
        a[i]=a[j];
        a[p]=x;
        i=i-1;
    }

    ret 0;
}

fn main()->i32
{   
    std::timer_start(27);
    N=10;
    let a: [i32; 11];
    a[0]=1;
    a[1]=3;
    a[2]=4;
    a[3]=7;
    a[4]=8;
    a[5]=11;
    a[6]=13;
    a[7]=18;
    a[8]=56;
    a[9]=78;
    let x:i32;
    let i:i32;
    i=0;
    x=std::getint();
    x=insert(a,x);
    while i<N
    {
        x = a[i];
        std::putint(x);
        x= 10;
        std::putch(x);
        i=i+1;
    }
    std::timer_stop(54);
    ret 0;
}