use std;

let N:i32;

let newline:i32;

fn mod(x:i32,y:i32)->i32
{
    ret x-(x/y)*y;
}

fn split(n:i32,a: [i32; 4])->i32
{
    let i:i32;
    i=N-1;
    while(i!=-1)
    {
        a[i]=mod(n,10);
        n=n/10;
        i=i-1;
    }

    ret 0;
}

fn main()->i32
{   
    std::timer_start(26);
    N=4;
    newline=10;
    let i:i32;
    let m:i32;
    let b: [i32; 4];
    m = 1478;
    m = split(m,b);
    let t:i32;
    i=0;
    while(i<4)
    {
        t=b[i];
        std::putint(t);
        std::putch(newline);
        i=i+1;
    }
    std::timer_stop(42);
    ret 0;
}
