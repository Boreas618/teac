fn main() -> int {
    let n: int = getint(); // 计算对于分母从2到12的所有最简真分数的数量

    let total_simplified_fractions: int = 0;
    let d: int = 2; // 开始的分母

    while(d <= n) {
        let count: int = 1; // 初始化互质计数（1总是互质）
        let i: int = 2; // 开始的可能互质的数

        while (i < d) {
            // 内部实现 gcd 函数的逻辑
            let  x: int = d;
            let  y: int = i;
            while (y != 0) {
                let temp:int = y;
                y = x-x/y*y;
                x = temp;
            }
            // 如果 gcd 为 1，计数增加
            if (x == 1) {
                count = count+1;
            }
            i =i+ 1; // 更新内层循环的索引
        }
        total_simplified_fractions =total_simplified_fractions + count;
        d =d+ 1; // 更新外层循环的索引
    }

    putint(total_simplified_fractions); // 输出所有最简真分数的总数量
    putch(10); // 输出换行
    ret 0;
}
