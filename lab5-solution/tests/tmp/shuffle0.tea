let hashmod:int;

let bucket[10000000]:int;
let head[10000000]:int;
let next[10000000]:int;
let nextvalue[10000000]:int;
let key[10000000]:int;
let value[10000000]:int;
let cnt:int;

let keys[10000000]:int;
let values[10000000]:int;
let requests[10000000]:int;
let ans[10000000]:int;

fn hash(k:int)->int{
    ret k-(k/hashmod)*hashmod;
}

fn insert(k:int, v:int)->int{
    let h:int = hash(k);
    if (head[h] == 0){
        cnt = cnt + 1;
        head[h] = cnt;
        key[cnt] = k;
        value[cnt] = v;
        next[cnt] = 0;
        nextvalue[cnt] = 0;
        ret 0;
    }
    let p:int = head[h];
    while (p != 0){
        if (key[p] == k){
            cnt = cnt + 1;
            nextvalue[cnt] = nextvalue[p];
            nextvalue[p] = cnt;
            value[cnt] = v;
            ret 1;
        }
        p = next[p];
    }
    cnt = cnt + 1;
    next[cnt] = head[h];
    head[h] = cnt;
    key[cnt] = k;
    value[cnt] = v;
    nextvalue[cnt] = 0;
    ret 0;
}

fn reduce(k:int)->int{
    let h:int = hash(k);
    let p:int = head[h];

    while (p != 0){
        if (key[p] == k){
            let ans:int = 0;
            let x:int = p;
            while (x != 0){
                ans = ans + value[x];
                x = nextvalue[x];
            }
            ret ans;
        }
        p = next[p];
    }
    ret 0;
}

fn main()->int{
    hashmod = getint();
    let ks:int = getint();
    let ii:int=0;
    while(ii<ks){
        keys[ii]=getint();
        ii=ii+1;
    }
    let vs:int = getint();
    ii=0;
    while(ii<vs){
        values[ii]=getint();
        ii=ii+1;
    }
    let ms:int = getint();
    ii=0;
    while(ii<ms){
        requests[ii]=getint();
        ii=ii+1;
    }
    _sysy_starttime(209);

    let i:int = 0;
    while (i < ks){
        insert(keys[i], values[i]);
        i = i + 1;
    }
    i = 0;
    while (i < ms){
        ans[i] = reduce(requests[i]);
        i = i + 1;
    }
    _sysy_stoptime(312);
    putarray(ms, ans);
    ret 0;
}
