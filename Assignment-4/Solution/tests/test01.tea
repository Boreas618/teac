let a[10]:int;

fn main()->int{
    _sysy_starttime(4);
    let i:int = 0;
    let sum:int = 0;
    while(i < 10)
    {
        a[i] = i;
        i = i + 1;
    }
    i = 0;
    while(i < 10)
    {
        sum = sum + a[i];
        i = i + 1;
    }
    putint(sum);
    _sysy_stoptime(19);
    ret 0;
}