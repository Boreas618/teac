struct MyStruct { 
    node:int,
    len:int  
}

let a:MyStruct;

fn foo(a:int, b:int)->int{
    ret a+1+b+2;
}

fn main(a:int, b:int)->int{
    let c:MyStruct;
    c.node = 3;
    a.len = 20;
    
    while(a > b)
    {
        c.node = c.node + foo(a, b);
        a = a - 2;
    }

    ret c.node;
}



