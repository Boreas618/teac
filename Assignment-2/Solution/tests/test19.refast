fn getint()->int;

fn putint(num:int);

fn putch(ch:int);

let parent[1005]:int;

fn find(root:int)->int{
if((parent[root]==root)){
ret root;
}else{
parent[root] = find(parent[root]);
ret parent[root];
}
}

fn merge(p:int, q:int){
let root_p:int;
let root_q:int;
root_p = find(p);
root_q = find(q);
if((root_p!=root_q)){
parent[root_q] = root_p;
}
}

fn main()->int{
let n:int = getint();
let m:int = getint();
let i:int;
let p:int;
let q:int;
i = 0;
while((i<n)){
parent[i] = i;
i = i+1;
}
i = 0;
while((i<m)){
p = getint();
q = getint();
merge(p, q);
i = i+1;
}
let clusters:int = 0;
i = 0;
while((i<n)){
if((parent[i]==i)){
clusters = clusters+1;
}
i = i+1;
}
putint(clusters);
ret 0;
}

